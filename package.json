{
  "name": "suncalc",
  "version": "1.7.0",
  "description": "A tiny JavaScript library for calculating sun/moon positions and phases.",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint index.js test.js",
    "test": "tap test.js",
    "cov": "tap test.js --cov",
    "build": "browserify -s suncalc index.js > suncalc.js",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "eslint": "^1.9.0",
    "eslint-config-mourner": "^1.0.1",
    "tap": "^2.2.0"
  },
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "array-bracket-spacing": 0
    }
  },
  "homepage": "https://github.com/mourner/suncalc",
  "keywords": [
    "sun",
    "astronomy",
    "math",
    "calculation",
    "sunrise",
    "sunset",
    "twilight",
    "moon",
    "illumination"
  ],
  "author": "Vladimir Agafonkin",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git://github.com/mourner/suncalc.git"
  }
}
